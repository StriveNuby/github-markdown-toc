{
  "manifest_version": 2,

  "name": "git readme read",
  "description": "git readme read",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/content-script.js"],
      "css": ["css/content.css"],
      "run_at": "document_start"
    }
  ],
  "background": {
    "script": ["js/background.js"]
  },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  }
}
